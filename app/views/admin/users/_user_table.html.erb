        <th>手機號碼</th>
        <th>地址</th>
        <th>操作選項</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= user.user_name %></td>
          <td><%= user.gender %></td>
          <td><%= user.phone %></td>
          <td><%= user.address %></td>
          <td>            
            <li><%= link_to "查看" %></li>
            <li><%= link_to "編輯" %></li>
            <li><%= link_to "刪除" %></li>
          </td>
        </tr>
      <% end %>

      <tr>
        <td>5</td>
